{
  "name": "proposal-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:type-check": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "server": "cd server && node index.js",
    "start": "npm run server & npm run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,css,scss,json,md}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "validate": "npm run lint && npm run type-check"
  },
  "dependencies": {
    "@editorjs/checklist": "^1.5.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/list": "^1.8.0",
    "@editorjs/table": "^2.2.2",
    "@mdi/font": "^7.3.67",
    "@tiptap/extension-text-style": "^2.1.12",
    "@tiptap/extension-underline": "^2.1.12",
    "@tiptap/pm": "^2.1.12",
    "@tiptap/starter-kit": "^2.1.12",
    "@tiptap/vue-3": "^2.1.12",
    "ag-grid-community": "^31.1.1",
    "ag-grid-vue3": "^31.1.1",
    "axios": "^1.9.0",
    "fabric": "^5.3.0",
    "gridstack": "^8.4.0",
    "html2canvas": "^1.4.1",
    "interactjs": "^1.10.26",
    "jspdf": "^3.0.1",
    "pdfjs-dist": "^5.2.133",
    "perfect-freehand": "^1.2.0",
    "pinia": "^2.1.7",
    "roboto-fontface": "*",
    "signature_pad": "^4.1.6",
    "sortablejs": "^1.15.2",
    "vue": "^3.4.38",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.4.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/fabric": "^5.3.6",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.69.5",
    "typescript": "^5.5.3",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.1.4"
  }
}
